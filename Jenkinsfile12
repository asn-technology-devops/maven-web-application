pipeline { 

agent any 

tools {

maven "maven3.8.6"
}//close tools

stages { 
stage('sourcecode') { 
steps {
git credentialsId: 'a959c0e9-2f79-4e41-92d7-6985cec4ef70', url: 'https://github.com/asn-technology-devops/maven-web-application.git'
}
}//close sourcecode
stage ('build'){
steps {
sh "mvn clean package"
}
}//close build
stage ('executesonar'){
steps{
sh "mvn clean install sonar:sonar"
}
} //close sonar
stage ('executeintonexus') {
steps{
sh "mvn clean deploy"
}
}//

}
}
